<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.runtime.remoting>
    <application>
      <client>
        <wellknown
          type="Common.IUserList, Common"
          url="tcp://localhost:9000/Server/User"
        />
        <wellknown
          type="Common.IOperation, Common"
          url="tcp://localhost:9000/Server/Operation"
        />
      </client>
      <channels>
        <channel ref="tcp" port="0">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>